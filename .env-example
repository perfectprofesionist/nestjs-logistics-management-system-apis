# Example .env file for Logistics Management System

# Database connection string (update with your credentials)
DATABASE_URL="mysql://root:@localhost:3306/db_name"

# Frontend URL (update as needed)
FRONTEND_URL="http://localhost:3000"

# JWT configuration (replace with your own secret in production)
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=1d

# Cookie settings
COOKIE_NAME=access_token
COOKIE_SECURE=false
COOKIE_HTTP_ONLY=true
COOKIE_SAME_SITE="lax"

# Server port
PORT=5000